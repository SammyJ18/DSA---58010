        "test_scores = [26, 49, 98, 87, 62, 75]\n",
        "\n",
        "sum_even_scores = 0\n",
        "\n",
        "for score in test_scores:\n",
        "    if score % 2 == 0:\n",
        "        sum_even_scores += score\n",
        "\n",
        "print(\"Sum of even test scores:\", sum_even_scores)\n"
